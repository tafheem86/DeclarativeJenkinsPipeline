pipeline {
	agent any
	stages {
		stage("Clean") {
		        steps {
			      echo "Clean app "
			}
		}
		stage("Compile") {
		        steps {
			      echo "Compile app "
			}
		}
		stage("Build") {
			steps {
			      echo "Building app "
			}
		}
		stage("Test") {
			steps {
			     echo "test app "
			}
		}
		stage("Package") {
			steps {
				echo "Package app "
			}
		}
		stage("Deploy") {
			steps {
				echo "Deploy app "
			}
		}
	}
}
